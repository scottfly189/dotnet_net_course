    "version": "0.2.0",
    "configurations": [
        {
            "name": "C#: Demo_01 [Default Configuration]",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "dotnet: build",
            "program": "${workspaceFolder}/bin/Debug/net9.0/Demo_01.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "justMyCode": false,
            "console": "internalConsole",
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "ASPNETCORE_URLS": "http://localhost:5175"
            },
            "pipeTransport": {
                "pipeProgram": "cmd",
                "pipeArgs": ["/c"],
                "debuggerPath": "${workspaceFolder}/../netcoredbg/netcoredbg.exe"
            },
            "logging": {
                "moduleLoad": false
            }
        }
    ]
}